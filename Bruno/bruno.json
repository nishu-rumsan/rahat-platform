{
  "version": "1",
  "name": "Rahat Core",
  "type": "collection",
  "ignore": [
    "node_modules",
    ".git"
  ],
  "scripts": {
    "moduleWhitelist": [
      "crypto",
      "util",
      "events",
      "net",
      "tls",
      "buffer"
    ],
    "filesystemAccess": {
      "allow": true
    },
    "polyfills": [
      "text-encoding"
    ]
  }
}